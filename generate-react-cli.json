{
  "usesTypeScript": false,
  "usesStyledComponents": false,
  "usesCssModule": true,
  "cssPreprocessor": "css",
  "testLibrary": "None",
  "component": {
    "default": {
      "customTemplates": {
        "component": "templates/component/TemplateName.jsx",
        "style": "templates/component/TemplateName.module.css"
       
      },
      "path": "src/Components",
      "withStyle": true,
      "withTest": false,
      "withStory": false,
      "withLazy": false
    },
    "page":{
    "customTemplates": {
      "component": "templates/component/TemplateName.jsx",
      "style": "templates/component/TemplateName.module.css"
    },
      "path": "src/pages",
      "withStyle": true
    }

  }
}